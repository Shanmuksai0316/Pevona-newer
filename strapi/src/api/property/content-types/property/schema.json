{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": ["For Sale", "For Rent", "Sold"],
      "required": true,
      "default": "For Sale"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "currency": {
      "type": "string",
      "default": "USD",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "zipcode": {
      "type": "string",
      "required": true
    },
    "bedrooms": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "bathrooms": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "area": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "short_description": {
      "type": "text"
    },
    "description": {
      "type": "richtext"
    },
    "features": {
      "type": "json"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "floor_plans": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "latitude": {
      "type": "decimal"
    },
    "longitude": {
      "type": "decimal"
    },
    "map_embed": {
      "type": "text"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}







